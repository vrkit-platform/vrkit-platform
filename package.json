  "scripts": {
    "compile": "tsc -b tsconfig.json",
    "compile:all": "run-s compile:tools compile",
    "compile:all:watch": "run-p compile:watch compile:tools:watch",
    "compile:tools": "yarn workspace vrkit-builder-tool build",
    "compile:tools:watch": "yarn workspace vrkit-builder-tool build",
    "compile:watch": "tsc -b tsconfig.json -w --preserveWatchOutput",
    "dev": "tsc-watch -b tsconfig.json  --preserveWatchOutput  --onFirstSuccess \"./scripts/dev.mjs\"",
    "postinstall": "patch-package && electron-builder.cmd install-app-deps && node ./scripts/postinstall.mjs",
    "vrkit-app:version-bump": "cross-env NODE_ENV=production node ./scripts/bump-platform-version.mjs",
    "vrkit-app:package": "cross-env NODE_ENV=production node ./scripts/build-packaged-app.mjs",
    "vrkit-app:dev": "yarn workspace vrkit-app run dev",
    "vrkit-app:start": "yarn workspace vrkit-app run start",
    "vrkit-app:start-brk": "yarn workspace vrkit-app run start:brk"
  },
