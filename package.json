{
  "name": "vrkit-project",
  "version": "1.0.0",
  "description": "VRKit Workspace Project",
  "workspaces": [
    "packages/js/*"
  ],
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "31.3.0",
    "arch": "x64"
  },
  "main": "build/js/vrkit-dist/main/main.js",
  "binary": {
    "napi_versions": [
      9
    ]
  },
  "scripts": {
    "compile": "tsc -b tsconfig.json",
    "compile:watch": "tsc -b tsconfig.json -w",
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:dll": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./etc/webpack/configs/webpack.config.renderer.dev.dll.ts",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./etc/webpack/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./etc/webpack/configs/webpack.config.renderer.prod.ts",
    "build:main:dev": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./etc/webpack/configs/webpack.config.main.dev.ts",
    "build:main:dev:watch": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./etc/webpack/configs/webpack.config.main.dev.ts --watch",
    "package": "cross-env ts-node ./etc/webpack/scripts/clean.js dist && npm run build && electron-builder build --publish never && npm run build:dll",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "cross-env ts-node ./etc/webpack/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./etc/webpack/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./etc/webpack/configs/webpack.config.renderer.dev.ts",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jglanz/irsdk-interop.git"
  },
  "private": true,
  "dependencies": {
    "bindings": "^1.5.0",
    "electron-debug": "^4.0.0",
    "prelude-ts": "^1.0.6",
    "lodash": "^4.17.21",
    "electron-log": "^5.1.7",
    "electron-updater": "^6.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-refresh": "0.14.2",
    "react-test-renderer": "18.3.1",
    "react-router-dom": "^6.25.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "cmake-js": "^7.3.0",
    "electron-rebuild": "^3.2.9",
    "node-addon-api": "^8.1.0",
    "@electron/rebuild": "^3.6.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@protobuf-ts/plugin": "^2.9.4",
    "@svgr/webpack": "^8.1.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/bindings": "^1.5.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "core-js": "^3.37.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "detect-port": "^1.6.1",
    "electron": "^31.3.0",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electronmon": "^2.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-refresh": "0.14.2",
    "react-test-renderer": "18.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.93.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "build/js/vrkit-dist/main/**/**"
    ],
    "logLevel": "quiet"
  },
  "resolutions": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-refresh": "0.14.2",
    "react-test-renderer": "18.3.1"
  }
}
