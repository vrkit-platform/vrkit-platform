{
  "name": "vrkit-project",
  "version": "1.0.0",
  "description": "VRKit Workspace Project",
  "workspaces": [
    "packages/js/*"
  ],
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "31.3.0",
    "arch": "x64"
  },
  "main": "packages/js/vrkit-app/dist/main/electron-main.js",
  "binary": {
    "napi_versions": [
      9
    ]
  },
  "scripts": {
    "compile": "tsc -b tsconfig.json",
    "compile:watch": "tsc -b tsconfig.json -w --preserveWatchOutput",
    "compile:tools": "yarn workspace vrkit-builder-tool build",
    "compile:tools:watch": "yarn workspace vrkit-builder-tool build",
    
    "compile:all": "run-s compile:tools compile",
    "compile:all:watch": "run-p compile:watch compile:tools:watch",
    
    "dev:vrkit-app": "yarn workspace vrkit-app run dev",
    "dev": "run-s compile:all && run-p compile:all:watch dev:vrkit-app",
    
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jglanz/irsdk-interop.git"
  },
  "private": true,
  "dependencies": {
    "bindings": "^1.5.0",
    "electron-debug": "^4.0.0",
    "electron-log": "^5.1.7",
    "electron-updater": "^6.3.1",
    "lodash": "^4.17.21",
    "prelude-ts": "^1.0.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-refresh": "0.14.2",
    "react-router-dom": "^6.26.0",
    "react-test-renderer": "18.3.1",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@protobuf-ts/plugin": "^2.9.4",
    "@svgr/webpack": "^8.1.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/bindings": "^1.5.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "@types/tinycolor2": "^1.4.6",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^5.3.0",
    "cmake-js": "^7.3.0",
    "concurrently": "^8.2.2",
    "core-js": "^3.38.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "detect-port": "^1.6.1",
    "ejs": "^3.1.10",
    "ejs-loader": "^0.5.0",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "@tomjs/electron-devtools-installer": "^2.3.2",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "electronmon": "^2.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "node-addon-api": "^8.1.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-refresh": "0.14.2",
    "react-test-renderer": "18.3.1",
    "rimraf": "^6.0.1",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.93.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "build/js/vrkit-dist/main/**/**"
    ],
    "logLevel": "quiet"
  },
  "resolutions": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-refresh": "0.14.2",
    "react-test-renderer": "18.3.1"
  }
}
