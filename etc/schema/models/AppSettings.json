{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/AppSettings",
    "definitions": {
        "AppSettings": {
            "properties": {
                "defaultDashboardConfigId": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "string"
                        }
                    ]
                },
                "themeType": {
                    "enum": [
                        "AUTO",
                        0,
                        "LIGHT",
                        1,
                        "DARK",
                        2
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "title": "Theme Type"
                },
                "zoomFactor": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "number"
                        }
                    ]
                },
                "autoconnect": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "boolean"
                        }
                    ]
                },
                "openAppOnBoot": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "boolean"
                        }
                    ]
                },
                "openDashboardOnLaunch": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "boolean"
                        }
                    ]
                },
                "customAccelerators": {
                    "additionalProperties": {
                        "oneOf": [
                            {
                                "type": "null"
                            },
                            {
                                "type": "string"
                            }
                        ]
                    },
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "object"
                        }
                    ]
                },
                "overlayAnchors": {
                    "additionalProperties": {
                        "enum": [
                            "OVERLAY_ANCHOR_CENTER",
                            0,
                            "OVERLAY_ANCHOR_TOP_LEFT",
                            1,
                            "OVERLAY_ANCHOR_TOP_RIGHT",
                            2,
                            "OVERLAY_ANCHOR_BOTTOM_RIGHT",
                            3,
                            "OVERLAY_ANCHOR_BOTTOM_LEFT",
                            4
                        ],
                        "oneOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "integer"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Overlay Anchor"
                    },
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "object"
                        }
                    ]
                }
            },
            "additionalProperties": true,
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "type": "object"
                }
            ],
            "title": "App Settings"
        }
    }
}